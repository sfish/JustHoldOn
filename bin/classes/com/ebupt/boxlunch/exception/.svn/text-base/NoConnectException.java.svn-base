package com.ebupt.boxlunch.exception;

public class NoConnectException extends Exception {

	public static final String NO_CONNECTION_EXCEPTION_CODE = "1985732875.234asdgw2351664643d";

	private static final long serialVersionUID = -519638538214334215L;

	public NoConnectException(String message) {
		super(message);
	}

	public NoConnectException(Throwable cause) {
		super(cause);
	}

	public NoConnectException(String message, Throwable cause) {
		super(message, cause);
	}

}
